<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<!--How to test two or more test together -->

<suite name="TestAutomation" verbose="1" parallel="classes" thread-count="3" >
    <test name="selenium_grid">
        <classes>
            <class name="selenium_grid.SeleniumGrid_Test1"/>
            <class name="selenium_grid.SeleniumGrid_Test2"/>
            <class name="selenium_grid.SeleniumGrid_Test3"/>
        </classes>
    </test>


</suite>
